{
  "name": "halo_lab_v3",
  "version": "0.0.1",
  "description": "Personal website for Halo Lab company",
  "scripts": {
    "start": "rimraf build; eleventy --serve --port=3000",
    "build": "rimraf build; NODE_ENV=production eleventy build",
    "serve": "http-server build --port 3000 -b --ssl --key certs/key.pem --cert certs/cert.pem",
    "certs": "mkdir -p certs; mkcert -key-file certs/key.pem -cert-file certs/cert.pem localhost 127.0.0.1"
  },
  "author": "Halo Lab team",
  "license": "UNLICENSED",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.8.2",
    "@quasibit/eleventy-plugin-sitemap": "^2.0.5",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "browserslist": "^4.16.3",
    "dotenv": "^8.2.0",
    "eleventy-plugin-compress": "^1.0.0",
    "eleventy-plugin-pwa-icons": "^1.0.0",
    "eleventy-plugin-scripts": "^1.0.0",
    "eleventy-plugin-styles": "^1.0.0",
    "eleventy-plugin-workbox": "^1.1.1",
    "eleventy-shortcode-image": "^1.0.1",
    "html-minifier": "^4.0.0",
    "http-server": "^0.12.3",
    "postcss-url": "^10.1.3",
    "prettier": "^2.2.1",
    "prettier-config-halo-lab": "^1.0.7",
    "rimraf": "^3.0.2",
    "svgo": "^2.3.0"
  },
  "prettier": "prettier-config-halo-lab",
  "browserslist": [
    "> 0.5%",
